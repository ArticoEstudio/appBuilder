<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
    <head>
        <title>.:: {{app.name}}</title>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">

        <script type="text/javascript" src="{{app.path}}/js/tabber.js"></script>
        <link rel="stylesheet" href="{{app.path}}/css/tabber.css" TYPE="text/css" MEDIA="screen">
    </head>
    <body>

        <div class="tabber">
            <div class="tabbertab">
                <h2>Sucursales</h2>
                <p>
                    <iframe width="100%" height="450" scrolling="yes" style="border: 0px dotted grey;"
                            src="{{app.path}}/Sucursales/list/{{values.idEmpresa}}">
                    </iframe>
                </p>
            </div>

            <div class="tabbertab">
                <h2>Almacenes</h2>
                <p>
                    <iframe width="100%" height="300" scrolling="yes" style="border: 0px dotted grey;"
                            src="{{app.path}}/EmpresasAlmacenes/list/{{values.idEmpresa}}">
                    </iframe>
                </p>
            </div>
        </div>
    </body>
</html>
